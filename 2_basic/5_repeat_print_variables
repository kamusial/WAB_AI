a = 5            #int
b= 7.5           #float
c = 'doggy'      #string

print(f'Variable a is {a}, variable b is {b}')
print(f'My word, variable c is {c}')

print(f'Some calculation, sum is: {a+b}')

my_list = [34, 12.6, 'doggy', 'computer', 1254]
print(f'first element is {my_list[0]}')   
print(f'3rd element is {my_list[2]}')
print(f'last element is {my_list[-1]}') #last element
